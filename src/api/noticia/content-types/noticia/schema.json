{
  "kind": "collectionType",
  "collectionName": "noticias",
  "info": {
    "singularName": "noticia",
    "pluralName": "noticias",
    "displayName": "Noticia",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titulo": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "titulo"
    },
    "subtitulo": {
      "type": "string"
    },
    "descripcion": {
      "type": "text"
    },
    "contenido": {
      "type": "text"
    },
    "imagenes": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "activa": {
      "type": "boolean",
      "default": true
    },
    "fecha": {
      "type": "datetime"
    },
    "isFeatured": {
      "type": "boolean"
    },
    "videos": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "imagenPrincipal": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
